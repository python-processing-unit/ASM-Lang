{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["[", "]"],
    ["{", "}"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "{", "close": "}" }
  ],
  "surroundingPairs": [
    { "open": "\"", "close": "\"" },
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "{", "close": "}" }
  ],
  "wordPattern": "[a-zA-Z_/.!@$%&~+|<>?\\\\-]+|[01]+",
  "folding": {
    "markers": {
      "start": "^\\s*\\[",
      "end": "^\\s*\\]"
    }
  }
}
